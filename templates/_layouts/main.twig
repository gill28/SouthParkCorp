										<button type="submit">
											<i class="ion-ios-search-strong"></i>
										</button>
									</div>
								</form>
							</div>
						</div>
						<div class="col-lg-3 col-md-4 col-12 text-right">
							<div class="mini-cart-area">
								<ul>
									{% set cart = craft.commerce.carts.cart %}
									<li>
										<a href="#">
											<i class="ion-android-cart"></i>
											<span class="cart-add">
												{{ craft.orders.number(cart.number).one().totalQty ?? 0}}
											</span>
											<span class="cart-total">
												$
												{{ cart.storedItemTotal | number_format(2, '.', ',') }}

												<i class="fa fa-angle-down"></i>
											</span>
										</a>
										<ul class="cart-dropdown">
											{% if cart.lineItems | length %}
												{% for item in cart.lineItems %}

													<li class="cart-item">
														<div class="cart-img">
															<a href="{{item.purchasable.product.url}}">
																<img src="{{item.purchasable.product.productImage.one.getUrl()}}" alt="">
															</a>
														</div>
														<div class="cart-content">
															<h4>
																<a href="{{item.purchasable.product.url}}">
																	{{item.purchasable.product.productTitle}}

																</a>
															</h4>
															<p class="cart-quantity">
																Qty:
																{{item.qty}}
															</p>
															<p class="cart-price">
																{{ item.price | currency(cart.currency) }}

															</p>
														</div>
													</li>
												{% endfor %}

												<li class="cart-total-amount mtb-20">
													<h4>
														SubTotal :
														<span class="pull-right">
															{{ cart.totalPrice | currency(cart.currency )}}
														</span>
													</h4>
												</li>


												<li class="cart-button">
													<a href="store/cart" class="button2">
														View cart
													</a>
													<a href="store/checkout" class="button2">
														Check out
													</a>
												</li>
											{% else %}
												<p>
													Cart is Emtpy
												</p>
											{% endif %}
										</ul>
									</li>
								</li>
							</li>
						</ul>
					</div>
				</div>
